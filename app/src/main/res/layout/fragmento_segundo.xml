<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <Button
        android:id="@+id/segundo"
        android:layout_width="213dp"
        android:layout_height="0dp"
        android:layout_marginBottom="224dp"
        android:backgroundTint="@color/purple_700"
        android:text="Proximo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/celular" />

    <EditText
        android:id="@+id/nacionalidade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="187dp"
        android:ems="10"
        android:hint="Nacionalidade"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/celular"
        app:layout_constraintStart_toStartOf="@+id/celular"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/celular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="89dp"
        android:layout_marginTop="80dp"
        android:ems="10"
        android:hint="Celular"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nacionalidade" />

</androidx.constraintlayout.widget.ConstraintLayout>